<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zhongji.master.android.phone"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="21" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.NoTitleBar" >
        
        <!-- 登陆 -->
        <activity
            android:name="com.zhongji.master.android.phone.login.LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
         <!-- 注册 -->
        <activity
            android:name="com.zhongji.master.android.phone.login.RegisterActivity"
            android:label="@string/app_name" >
        </activity>
        
          <!-- 找回密码 -->
        <activity
            android:name="com.zhongji.master.android.phone.login.FoundPasswordActivity"
            android:label="@string/app_name" >
        </activity>
        
           <!-- 随便 -->
        <activity
            android:name="com.zhongji.master.android.phone.login.AAA"
            android:label="@string/app_name" >
        </activity>
        
    </application>

</manifest>
